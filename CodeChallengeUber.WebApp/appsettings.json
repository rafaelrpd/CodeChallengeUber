{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OlhoVivoApiSettings": {
    "Endpoints": {
      "Autenticar": "/Login/Autenticar?token={token}",
      "BuscarLinhas": "/Linha/Buscar?termosBusca={termosBusca}",
      "BuscarLinhaSentido": "/Linha/BuscarLinhaSentido?termosBusca={termosBusca}&sentido={sentido}",
      "BuscarParadas": "/Parada/Buscar?termosBusca={termosBusca}",
      "BuscarParadasPorLinha": "/Parada/BuscarParadasPorLinha?codigoLinha={codigoLinha}",
      "BuscarParadasPorCorredor": "/Parada/BuscarParadasPorCorredor?codigoCorredor={codigoCorredor}",
      "GetCorredores": "/Corredor",
      "GetEmpresas": "/Empresa",
      "GetPosicao": "/Posicao",
      "GetPosicaoLinha": "/Posicao/Linha?codigoLinha={codigoLinha}",
      "GetPosicaoGaragem": "/Posicao/Garagem?codigoEmpresa={codigoEmpresa}&codigoLinha={codigoLinha}",
      "GetPrevisaoChegada": "/Previsao?codigoParada={codigoParada}&codigoLinha={codigoLinha}",
      "GetPrevisaoLinha": "/Previsao/Linha?codigoLinha={codigoLinha}",
      "GetPrevisaoParada": "/Previsao/Parada?codigoParada={codigoParada}",
      "GetVelocidadeVias": "/KMZ",
      "GetVelocidadeCorredor": "/KMZ/Corredor",
      "GetVelocidadeOutrasVias": "/KMZ/OutrasVias"
    }
  }
}